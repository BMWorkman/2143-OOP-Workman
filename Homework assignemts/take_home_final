  class Point:
    def __init__(self,x,y):
        self.x = x
        self.y = y

    def __str__(self):
        return "[%d,%d]" % (self.x, self.y) 

class Shape(object):
    def __init(self,p1,p2):
        self.p1 = p1
        self.p2 = p2
    
    def area(self):
        return self.p1 * self.p2

class Square(object):
    def __init(self,x):
      self.x = x
    def area(self):
      self.x ** 2
    def perameter(self):
      self.x * 4
      

class Rectangle(object):
  def __init(self): 
    pass
  def area(self):
    return self.side.p1 * self.side.p2
  def perameter(self):
      return (self.side.p1 +self.side.p2) * 2

class Cube(object):
  def __init(self):
    pass
  def surfaceArea(self):
    return (self.side.p1 **2) * 6
  def volume(self):
    self.shape ** 3
  
    
